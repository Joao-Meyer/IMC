<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="br.senai.sp.jandira.imc">

    <application
        android:allowBackup="true"
        android:icon="@drawable/fit_amarelo_fundo_laranja"
        android:label="@string/app_name"
        android:roundIcon="@drawable/fit_amarelo_fundo_laranja"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <activity android:name=".IMCActivity" android:parentActivityName=".CalculadoraSaudavelActivity"></activity>

        <activity android:name=".ResultadoIMCActivity" android:parentActivityName=".IMCActivity"></activity>

        <activity android:name=".CalculadoraSaudavelActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>